    <script type="text/javascript">
        var imNotARobot = function () {
            $("#errortxtRecaptchaVerification").html("");
        };
        $(document).ready(function () {
            $(".mobileMenu").click(function () {
                $("nav ul").slideToggle('fast');
            });
        });

        $(function () {
            var windowHeight;

            $(window).on("scroll", function () {

                $('.topBar').css('min-height', windowHeight - 40);

                if ($(window).scrollTop() > 30) {
                    $(".navbar-trans").css("background-color", "#454545");
                }
                else {
                    $(".navbar-trans").css("background-color", "rgba(0,0,0,0.5)");
                }
            });

            function setHeight() {
                windowHeight = $(window).innerHeight();
                $('.topBar').css('min-height', windowHeight - 40);
            };
            setHeight();

            $(window).resize(function () {
                setHeight();
            });
        });

        function setFbCount(count) {
            var likeCount = addCommas(count);
            $('#<%=lblFacebokkLike.ClientID%>').html(likeCount);
        }

        var sess_expirationMinutes = 20;
        var sess_warningMinutes = 17;
        var sess_expireInterval;
        var sess_expireAlert;
        var SessionTimeOutflag = false;

        function initSession() {
            if ($("#hdnSessionFlag").val() == "true") {
                sess_expireInterval = window.setInterval('sessLogOut()', 1200000); //session expire in 20 minutes 1200000
                sess_expireAlert = window.setInterval('sessInterval()', 1020000); //session expire warning in 17 minutes 1020000
                $(document).bind('keypress.session', function (ed, e) {
                    sessKeyPressed(ed, e);
                });
            }
        }

        function sessKeyPressed(ed, e) {
            clearInterval(sess_expireInterval);
            clearInterval(sess_expireAlert);
            sess_expireInterval = window.setInterval('sessLogOut()', 1200000); //session expire in 20 minutes 1200000
            sess_expireAlert = window.setInterval('sessInterval()', 1020000); //session expire warning in 17 minutes 1020000
        }

        function sessLogOut() {
            SessionTimeOutflag = true;
            window.location.href = "../Pages/index.aspx";
        }

        function sessInterval() {
            var errorMsg = 'Your session will expire in ' + (sess_expirationMinutes - sess_warningMinutes) + ' minutes. <br/> Please save your data.';
            $("#SessionContent").html(errorMsg);
            $('#SessionModal').modal('show');
        }
        $(function () {
            initSession();
        });

    </script>
